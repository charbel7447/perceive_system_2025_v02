{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1 | ^8.2",
        "ext-curl": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mysqli": "*",
        "ext-openssl": "*",
        "ext-zip": "*",
        "barryvdh/laravel-dompdf": "^2.0",
        "box/spout": "*",
        "brian2694/laravel-toastr": "^5.56",
        "ckeditor/ckeditor": "^4.17",
        "consoletvs/charts": "*",
        "doctrine/dbal": "^3.2",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "gregwar/captcha": "^1.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "kingflamez/laravelrave": "^4.2",
        "kwn/number-to-words": "*",
        "laravel/framework": "^8.65",
        "laravel/helpers": "*",
        "laravel/passport": "^10.2",
        "laravel/sanctum": "^2.11",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "*",
        "laravelpkg/laravelchk": "dev-master",
        "lcobucci/jwt": "^4.1",
        "league/flysystem-aws-s3-v3": "*",
        "maatwebsite/excel": "^3.1",
        "madnest/madzipper": "^1.1",
        "mercadopago/dx-php": "^2.4",
        "milon/barcode": "^8.0",
        "mpdf/mpdf": "^8.0",
        "nexmo/laravel": "^2.4",
        "nwidart/laravel-modules": "8.2",
        "paypal/rest-api-sdk-php": "^1.14",
        "rachidlaasri/laravel-installer": "*",
        "rap2hpoutre/fast-excel": "^3.1",
        "riskihajar/terbilang": "*",
        "rmunate/spell-number": "^4.0",
        "seshac/laravel-shiprocket-api": "^2.0",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-backup": "^7.8",
        "spatie/laravel-sitemap": "^5.9",
        "stripe/stripe-php": "^7.107",
        "symfony/translation": "^5.4"
    },
    "require-dev": {
        "amirami/localizator": "^0.7.1@alpha",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Support/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
